
@model List<string>
<section class="page-header-section style-1" style="background:url(~/assets/images/page-header.jpg)">
    <div class="container">
        <div class="page-header-content">
            <div class="page-header-inner">
                <div class="page-title">
                    <h2>TrueMatch Find Your Dating Partner</h2>
                </div>
                <ol class="breadcrumb">
                    <li><a asp-action="Index" asp-controller="Home">Home</a></li>
                    <li class="active">Members</li>
                </ol>
            </div>
        </div>
    </div>
</section>
<table class="table table-dark table-striped-columns">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Email</th>
            <th scope="col">Message</th>
        </tr>
    </thead>
    <tbody>
        @foreach( var msg in Model)
        {
            <tr>
                <td>#</td>
                <td>@msg</td>
                <td><a class="btn btn-primary" asp-action="Chat" asp-route-withUser="@msg"> Message</a></td>
            </tr>
        }

    </tbody>
</table>